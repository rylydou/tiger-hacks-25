; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[accessibility]

general/accessibility_support=2

[addons]

ss2d/version="3.3.0"

[application]

config/name="Tiger Hacks 25"
run/main_scene="uid://cobt7kj4b7bjl"
config/features=PackedStringArray("4.5")
config/icon="res://icon.svg"

[audio]

soundscaper/soundbank_dir="res://resources/soundbank"
soundscaper/todo_file="user://sound_event_todo.txt"
soundscaper/todo_file.editor="user://sound_event_todo.txt"

[autoload]

DevTools="*res://addons/devtools/autoload.tscn"
SFX="*res://addons/soundscaper/scripts/sfx.gd"
Inventory="*res://scripts/static/items/inventory.gd"

[debug]

gdscript/warnings/enable=false
shader_language/warnings/enable=false
shapes/collision/shape_color=Color(0, 0.6013788, 0.70120233, 0)

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor]

naming/node_name_num_separator=1

[editor_plugins]

enabled=PackedStringArray("res://addons/devtools/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg", "res://addons/run_preset/plugin.cfg", "res://addons/soundscaper/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://content/": "red",
"res://resources/": "blue",
"res://scenes/": "yellow",
"res://scripts/": "green"
}

[filesystem]

import/fbx2gltf/enabled=false
import/blender/enabled=false

[gui]

common/snap_controls_to_pixels=false

[physics]

2d/default_gravity=0.0

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
textures/vram_compression/import_s3tc_bptc=true
textures/vram_compression/import_etc2_astc=true
