script/source = "extends Node2D


func _process(delta: float) -> void:
	if Input.is_action_just_pressed(&\"ui_accept\"):
		take_in_game_screenshot()

# A function to capture and save the screenshot
func take_in_game_screenshot() -> void:
	# 1. Get the current viewport and its texture
	var viewport = get_viewport()
	var texture = viewport.get_texture()
	
	# 2. Get the image data from the texture
	var image = texture.get_image()
	
	# 3. Define the save path (e.g., \"user://screenshots/screenshot_01.png\")
	var save_path = \"user://screenshots/screenshot_.png\"
	# Ensure the \"screenshots\" directory exists
	DirAccess.make_dir_recursive_absolute(\"user://screenshots/\")
	
	# 4. Save the image
	var err = image.save_png(save_path)
	
	# Optional: Check for errors
	if err != OK:
		print(\"Error saving screenshot: \", err)
	else:
		print(\"Screenshot saved to: \", save_path)
"

